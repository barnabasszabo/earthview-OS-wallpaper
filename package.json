{
  "name": "earthview-os-wallpaper",
  "version": "1.0.2",
  "description": "Earthview wallpaper rotation app",
  "homepage": "http://szabobarnabas.hu",
  "author": {
    "name": "Barnabas Szabo",
    "email": "git@szabobarnabas.hu"
  },
  "keywords": [
    "angular",
    "angular 11",
    "electron",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build:orig": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build:orig -- -c dev",
    "build:prod": "npm run build:orig -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "release": "npm run electron:build",
    "build": "npm run release"
  },
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/forms": "^11.0.0 || ^12.0.0-0",
    "@ng-bootstrap/ng-bootstrap": "9.0.2",
    "bluebird": "^3.7.2",
    "jimp": "0.16.1",
    "lodash": "4.17.20",
    "ngx-countdown": "11.0.1",
    "node-fetch": "^2.6.1",
    "wallpaper": "^4.4.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/build-angular": "0.1101.3",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "11.1.3",
    "@angular/common": "11.1.2",
    "@angular/compiler": "11.1.2",
    "@angular/compiler-cli": "11.1.2",
    "@angular/core": "11.1.2",
    "@angular/forms": "11.1.2",
    "@angular/language-service": "11.1.2",
    "@angular/platform-browser": "11.1.2",
    "@angular/platform-browser-dynamic": "11.1.2",
    "@angular/router": "11.1.2",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.6.3",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.25",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/eslint-plugin-tslint": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "chai": "4.3.0",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.8.3",
    "cross-env": "7.0.3",
    "electron": "11.2.2",
    "electron-builder": "22.9.1",
    "electron-reload": "1.5.0",
    "eslint": "7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "~6.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.3",
    "spectron": "13.0.0",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3",
    "wait-on": "5.2.1",
    "webdriver-manager": "12.1.8",
    "zone.js": "0.11.3"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "chrome 83"
  ]
}
